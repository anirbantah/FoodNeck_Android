                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginTop="12dp" >

                    <TextView
                        android:id="@+id/textView43"
                        android:layout_width="150dp"
