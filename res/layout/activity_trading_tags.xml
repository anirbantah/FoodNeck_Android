                        android:layout_centerVertical="true"
                        android:layout_marginRight="8dp"
                        android:gravity="center_vertical|left"
                        android:paddingLeft="10dp"
                        android:text="Cards"
                        android:textColor="#AFAFAF" />

                    <EditText
                        android:id="@+id/editText15"
                        android:layout_width="60dp"
